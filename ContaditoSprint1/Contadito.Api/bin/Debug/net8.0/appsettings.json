{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Jwt": {
    "Issuer": "contadito",
    "Audience": "contadito-users",
    "Key": "REPLACE_WITH_A_LONG_RANDOM_SECRET_KEY",
    "ExpiresMinutes": 240
  },
  "ConnectionStrings": {
    "Default": "server=localhost;port=3306;database=contadito;user=root;password=admin;"
  },
  "AllowedHosts": "*"
}